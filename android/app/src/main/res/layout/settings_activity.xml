<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal">

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/textView"
                android:layout_width="156dp"
                android:layout_height="wrap_content"
                android:text="Model Name" />

            <Spinner
                android:id="@+id/spinner"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Input Image Width" />

            <TextView
                android:id="@+id/model_width"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="undefined" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Input Image Height" />

            <TextView
                android:id="@+id/model_height"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="undefined" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Rescaling Factor" />

            <TextView
                android:id="@+id/rescaling_factor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="undefined" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Use NNAPI" />

            <TextView
                android:id="@+id/use_nnapi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="undefined" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Model Rescales" />

            <TextView
                android:id="@+id/model_rescales"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="undefined" />

        </TableRow>

    </TableLayout>

</LinearLayout>
